#include <stdio.h>
int isPrint(int m)
#include <math.h>

int main(void)
{
    int m, plag, count = 0;
    for (m = 2; m <= 100; m++)
    {
        plag = isPrint(m);
        if (plag == 1)
        {
            printf("%5d", m);
            count++;
            if (count % 5 == 0)
            {
                printf("\n");
            }
        }
    }
   


{
    int i;
    for (i = 2; i <= sqrt(m); i++)
    {
        if (m % i == 0)
        {
            return 0;
        }
    }
    return 1;
}
}
